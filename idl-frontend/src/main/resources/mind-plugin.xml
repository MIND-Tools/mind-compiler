<plugin
    id="org.ow2.mind.idl-frontend"
    name="The IDL Frontend of the MIND Compiler">

	<extension
        point="org.ow2.mind.predefined-annotations">
        <annotation
            package="org.ow2.mind.idl.annotation.predefined" />
    </extension>

    <extension
        point="org.ow2.mind.plugin.guice-module">
        <module
            class="org.ow2.mind.idl.IDLFrontendModule">
            <combine
                class="org.ow2.mind.CommonFrontendModule" />
        </module>
    </extension>
</plugin>
