<?xml version='1.0'?>
<highlighters>

<highlighter type='multiline-comment'>
  <start>/*</start>
  <end>*/</end>
</highlighter>

<highlighter type='oneline-comment'>
  //
</highlighter>

<highlighter type='string'>
  <string>"</string>
  <escape>\</escape>
</highlighter>

<highlighter type='heredoc'>
  <start>{{</start>
  <end>}}</end>
</highlighter>



<highlighter type='keywords'>
  <keyword>abstract</keyword>
  <keyword>as</keyword>
  <keyword>attribute</keyword>
  <keyword>binds</keyword>
  <keyword>composite</keyword>
  <keyword>conformsto</keyword>
  <keyword>contains</keyword>
  <keyword>data</keyword>
  <keyword>extends</keyword>
  <keyword>import</keyword>
  <keyword>int</keyword>
  <keyword>mandatory</keyword>
  <keyword>nodata</keyword>
  <keyword>optional</keyword>
  <keyword>primitive</keyword>
  <keyword>provides</keyword>
  <keyword>requires</keyword>
  <keyword>source</keyword>
  <keyword>string</keyword>
  <keyword>this</keyword>
  <keyword>to</keyword>
  <keyword>type</keyword>
</highlighter>

</highlighters>
